{
	"template": "gcp-flowlogs-*",
	"settings": {
		"refresh_interval": "5s"
	},
	"mappings": {
		"_default_": {
			"dynamic_templates": [{
				"message_field": {
					"path_match": "message",
					"mapping": {
						"norms": false,
						"type": "text"
					},
					"match_mapping_type": "string"
				}
			}, {
				"string_fields": {
					"mapping": {
						"norms": false,
						"type": "text",
						"fields": {
							"keyword": {
								"ignore_above": 256,
								"type": "keyword"
							}
						}
					},
					"match_mapping_type": "string",
					"match": "*"
				}
			}],
			"_all": {
				"norms": false,
				"enabled": false
			},
			"properties": {
				"insertId": {
					"type": "keyword"
				},
				"bytes_sent": {
					"type": "byte"
				},
				"conn.dest_ip": {
					"type": "ip"
				},
				"conn.dest_port": {
					"type": "integer"
				},
				"conn.protocol": {
					"type": "integer"
				},
				"conn.src_ip": {
					"type": "ip"
				},
				"conn.src_port": {
					"type": "integer"
				},
				"conn.disposition": {
					"type": "keyword"
				},
				"instance.project_id": {
					"type": "keyword"
				},
				"instance.region": {
					"type": "keyword"
				},
				"instance.vm_name": {
					"type": "keyword"
				},
				"instance.zone": {
					"type": "keyword"
				},
				"remote.continent": {
					"type": "keyword"
				},
				"remote.country": {
					"type": "keyword"
				},
				"rule.action": {
					"type": "keyword"
				},
				"rule.direction": {
					"type": "keyword"
				},
				"rule.ip_port": {
					"type": "integer"
				},
				"rule.priority": {
					"type": "integer"
				},
				"vpc.project_id": {
